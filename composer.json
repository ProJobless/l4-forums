{
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "villimagg/Laravel-MtHaml",
                "version":"master",
                "source": {
                    "url": "https://github.com/villimagg/Laravel-MtHaml.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        }
    ],
    "require": {
        "laravel/framework": "4.0.*",
        "villimagg/Laravel-MtHaml": "master"
